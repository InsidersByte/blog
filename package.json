{
  "name": "gatsby-blog-starter-kit",
  "description": "Gatsby blog starter example",
  "version": "1.1.0",
  "author": "Jonathon Kelly <jonathon@insidersbyte.com>",
  "dependencies": {
    "gatsby": "1.9.109",
    "gatsby-link": "1.6.26",
    "gatsby-plugin-catch-links": "1.0.9",
    "gatsby-plugin-react-helmet": "1.0.8",
    "gatsby-plugin-react-next": "1.0.4",
    "gatsby-plugin-sharp": "1.6.20",
    "gatsby-plugin-styled-components": "^2.0.2",
    "gatsby-remark-images": "1.5.31",
    "gatsby-source-filesystem": "1.5.8",
    "gatsby-transformer-remark": "1.7.20",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "react-helmet": "^5.2.0",
    "react-icons": "2.2.7",
    "styled-components": "^2.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "^0.59.0",
    "gh-pages": "1.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "prettier": "1.8.2"
  },
  "keywords": ["gatsby"],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "predeploy": "yarn build --prefix-paths",
    "postdeploy": "gh-pages -d public",
    "format": "prettier --write \"*.{js,json,css}\" \"**/*.{js,json,css,md}\"",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern flow-typed .",
    "test": "yarn run lint && flow"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
