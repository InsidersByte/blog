{
  "name": "@insidersbyte/blog",
  "description": "Jonathon Kelly's private blog",
  "version": "1.0.0",
  "author": "Jonathon Kelly <jonathon@insidersbyte.com>",
  "private": true,
  "dependencies": {
    "gatsby": "1.9.128",
    "gatsby-image": "1.0.27",
    "gatsby-link": "1.6.30",
    "gatsby-plugin-catch-links": "1.0.13",
    "gatsby-plugin-feed": "1.3.14",
    "gatsby-plugin-manifest": "1.0.9",
    "gatsby-plugin-netlify": "1.0.9",
    "gatsby-plugin-nprogress": "1.0.8",
    "gatsby-plugin-offline": "1.0.11",
    "gatsby-plugin-react-helmet": "2.0.2",
    "gatsby-plugin-react-next": "1.0.5",
    "gatsby-plugin-sharp": "1.6.22",
    "gatsby-plugin-sitemap": "1.2.6",
    "gatsby-plugin-styled-components": "2.0.3",
    "gatsby-plugin-twitter": "1.0.13",
    "gatsby-plugin-typography": "1.7.10",
    "gatsby-remark-autolink-headers": "1.4.9",
    "gatsby-remark-copy-linked-files": "1.5.22",
    "gatsby-remark-emoji": "0.0.1",
    "gatsby-remark-external-links": "0.0.3",
    "gatsby-remark-images": "1.5.33",
    "gatsby-remark-prismjs": "1.2.10",
    "gatsby-remark-smartypants": "1.4.9",
    "gatsby-source-filesystem": "1.5.9",
    "gatsby-transformer-remark": "1.7.23",
    "gatsby-transformer-sharp": "1.6.15",
    "param-case": "2.1.1",
    "prismjs": "1.9.0",
    "raven-js": "3.20.1",
    "react": "16.2.0",
    "react-aux": "1.1.0",
    "react-dom": "16.2.0",
    "react-helmet": "5.2.0",
    "react-icons": "2.2.7",
    "styled-components": "2.2.4",
    "typography": "0.16.6",
    "typography-theme-github": "0.15.10"
  },
  "devDependencies": {
    "babel-eslint": "8.0.3",
    "enzyme": "3.2.0",
    "enzyme-to-json": "3.2.2",
    "eslint": "4.12.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "flow-bin": "0.60.1",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "lint-staged": "6.0.0",
    "prettier": "1.9.1",
    "stylelint": "8.3.1",
    "stylelint-config-standard": "18.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.2.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:js": "eslint --ignore-path .gitignore --ignore-pattern flow-typed .",
    "lint": "yarn run lint:css && yarn run lint:js",
    "test": "jest --env=jsdom --watch",
    "format":
      "prettier --write \"*.{js,json,css,md}\" \"**/*.{js,json,css,md}\""
  },
  "jest": {
    "collectCoverageFrom": ["src/**/*.js"],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js",
      "<rootDir>/src/**/?(*.)(spec|test).js"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "snapshotSerializers": ["<rootDir>/node_modules/enzyme-to-json/serializer"]
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
